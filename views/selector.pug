extends layout

block content
  h1= title
  - var image_names = ["organisation1.jpeg","organisation2.jpeg","organisation3.jpeg","organisation4.jpeg","organisation5.jpeg"];
  - for(i=0;i<3;i++) {
     - var randNum = Math.floor(Math.random()*5) ;
     img(src=`./images/${image_names[randNum]}` alt=`img${image_names[randNum]}` style='width:300px; height:300px;')

  -}
